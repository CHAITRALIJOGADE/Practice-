Print Product of Elements in an Array


import java.util.Scanner;

public class ProductOfArrayElements {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        int product = 1;

        for (int i = 0; i < n; i++) {
            product *= sc.nextInt();
        }

        System.out.print(product);
        sc.close();
    }
}
